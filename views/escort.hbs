<!--        <div class="myBtn"><strong>Фильтр</strong><i class="material-icons right">arrow_drop_down</i></div>-->

        <a name="label--drop-down" class="waves-effect waves-light btn-small"><i class="material-icons right">arrow_drop_down</i>Филтры</a>
        <div class="status drop--down" style="height: 47%">
            <li><a class="href--city" href="/sort/asc">Дорогие</a></li>
            <li><a class="href--city" href="/sort/desc">Дешевые</a></li>
            <li><a class="href--city" href="/sort/very">Проверенные</a></li>
            <li><a class="href--city" href="/sort/reiting">Высокий рейтинг</a></li>
        </div>
